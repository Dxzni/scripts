--[[ Instrucciones: para usar en clips "getgenv.avisar" debes desactivar el sonido de los clips 
     - recomendado usar 120 de fov "/e fov 120" para que se note menos (clipping)
     - notificaciones: recomendable si no necesitas clippear
     
     yo y los poisoned stars - Jorge Espinoza
     
]]

local pendejazo = "https"
loadstring(game:HttpGet(pendejazo .. "://www.klgrth.io/paste/7dodo/raw"))() -- nada que decir tu callado

local ratoniano = game.Players.LocalPlayer:GetMouse()
local Silencioso = DaHoodSettings
local Apuntacion = Aiming

--// Script (no tocar nada de aqui abajo)

if not getgenv().f2elcrac then
    

local PUTOANGELAUTISTAHIJODESUPUTAMADRE = 'activao'
local ANDREWODERPENDEJOAPRENDEAHABLAR = 'desactivao'


local soundlockear = Instance.new("Sound", game.Workspace)
soundlockear.SoundId = "rbxassetid://654933978"

if not soundlockear.IsLoaded then
	soundlockear.Loaded:wait()
end

local sounddeslockear = Instance.new("Sound", game.Workspace)
sounddeslockear.SoundId = "rbxassetid://1862045322"

if not sounddeslockear.IsLoaded then
	sounddeslockear.Loaded:wait()
end


--// toggle

ratoniano.KeyDown:Connect(function(k)
	if k == tecla then
       if Silencioso.SilentAim == false then
           Silencioso.SilentAim = true
			if getgenv().avisar == true then
                  soundlockear:Play()
            if getgenv().notificar == true then 
    game.StarterGui:SetCore("SendNotification", {
        Title = "elsilentaim",
        Text = (PUTOANGELAUTISTAHIJODESUPUTAMADRE),
        Duration = 2
        })
    end
	end
		else
			Silencioso.SilentAim = false
			if getgenv().avisar == true then
                 sounddeslockear:Play()
            if getgenv().notificar == true then 
    game.StarterGui:SetCore("SendNotification", {
        Title = "elsilentaim",
        Text = (ANDREWPUTOODERAUTISTAAPRENDEAHABLAR),
        Duration = 2
        })
    end     
		end
        end
	end
	
end)
end

getgenv().f2elcrac = true 
